            <Font size="25.0" />
         </font>
      </Label>
      <VBox layoutX="53.0" layoutY="111.0" prefHeight="192.0" prefWidth="478.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefHeight="64.0" prefWidth="144.0" text="Pharmacy Name:" />
                  <TextField fx:id="pharmacyName" editable="false" prefHeight="25.0" prefWidth="230.0">
                     <HBox.margin>
                        <Insets left="10.0" top="21.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="nameButton" mnemonicParsing="false" onAction="#handleNameButton" text="Edit">
                     <HBox.margin>
                        <Insets left="20.0" top="21.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label alignment="CENTER_RIGHT" prefHeight="64.0" prefWidth="144.0" text="Pharmacy Address:" />
