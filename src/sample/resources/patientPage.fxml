         <font>
            <Font size="20.0" />
         </font></Label>
      <ScrollPane layoutX="20.0" layoutY="68.0" prefHeight="276.0" prefWidth="279.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="275.0" prefWidth="278.0" />
        </content>
      </ScrollPane>
      <Label layoutX="318.0" layoutY="193.0" prefHeight="36.0" prefWidth="102.0" text="Doctor Info:" textFill="#1427f8">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="318.0" layoutY="257.0" text="Nurse Info:" textFill="#1427f8">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="logoutButton" layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#handleLogoutButton" text="Logout" />
   </children>
</AnchorPane>
