      <Button fx:id="scheduleButton" layoutX="318.0" layoutY="101.0" mnemonicParsing="false" onAction="#handleScheduleAppointment" text="Schedule and View Appointment" />
      <Button fx:id="pharmacyInfoButton" layoutX="318.0" layoutY="136.0" mnemonicParsing="false" onAction="#handlePharmacyInfo" text="Edit and View Pharmacy Info" />
      <Button fx:id="labResultsButton" layoutX="318.0" layoutY="168.0" mnemonicParsing="false" onAction="#handleLabResults" text="Lab Results" />
